extends layout.pug

block content 
  nav.grid-container.portfolio-breadcrumb
    a(href='/') ‚Üê Back
  article.grid-container.portfolio-intro.portfolio-project
    .grid-x
      .cell.small-6.medium-9.large-9
        h1 #{projectName}
        p.lead.text-light
          | #{description}
      .cell.portfolio-meta.small-6.medium-2.large-3
        h6 Technologies
        ul
          each technology in technologies
            li= technology
        a.button.expanded(target="_blank" href=`${live}`) Live Demo
        a.button.expanded(target="_blank" href=`${gitHub}`) GitHub Repo
        br
    a(target="_blank" href=`${live}`)
      img.image(src=`${imgOne}`)
      br
    a(target="_blank" href=`${gitHub}`)  
      img.image(src=`${imgTwo}`)
